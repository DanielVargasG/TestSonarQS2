<!DOCTYPE xml>
<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<P13nDialog ok="onOK" cancel="onCancel" class="sapUiSizeCompact">
		<panels>
			<P13nColumnsPanel  items="{path: '/ColumnCollection'}" columnsItems="{path: '/ColumnsItems'}">
				<items>
					<P13nItem columnKey="{columnKey}" text="{text}" />
				</items>
				<columnsItems>
					<P13nColumnsItem columnKey="{columnKey}" index="{index}" visible="{visible}" />
				</columnsItems>
			</P13nColumnsPanel>
			
		</panels>
	</P13nDialog>
</core:FragmentDefinition>
<!DOCTYPE xml>
<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="generator.view.Menu.AdministrationAttachment" xmlns:html="http://www.w3.org/1999/xhtml" setDisplayBlock="true">
	<Page showNavButton="false" title="{i18n>DEFAULT_DETAILS_PAGE_NAME}">
		<customHeader showNavButton="true" navButtonTap="onNavPressed">
			<Bar design="Header">
				<contentLeft>
					<Button id="backButton" press="onNavPressed" icon="sap-icon://nav-back" />
				</contentLeft>
				<contentMiddle>
					<Image src="./sap/fiori/ppldoc/logo.png" height="30px" />
				</contentMiddle>
				<contentRight>
				</contentRight>
			</Bar>
		</customHeader>
		<content>
			<TileContainer id="container" tileDelete="handleTileDelete">								
				<StandardTile press="onPressAttachmentToProcess" icon="sap-icon://instance" title="{i18n>adminatt_Synchronizations_process}" />	
				<StandardTile press="onPressAttachmentHistory" icon="sap-icon://sap-box" title="{i18n>adminatt_synchronizations_history}" />			
			</TileContainer>
		</content>
		
	</Page>
</core:View>